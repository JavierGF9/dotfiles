Xcursor.theme: capitaine-cursors

xft*dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
Xft.hinting: true
Xft.autohint: true
Xft.hintstyle: hintfull
Xft.antialias: true

URxvt.font: xft:IBM 3270:style=Medium
URxvt.letterSpace: -2
URxvt.scrollBar: False
URxvt.cursorBlink: True

! gruvbox theme
*.foreground:   #ebdbb2
*.background:   #282828
*.cursorColor:  #ebdbb2

! black
*.color0:       #282828
*.color8:       #504945

! red
*.color1:       #cc241d
*.color9:       #fb4934

! green
*.color2:       #98971a
*.color10:      #b8bb26

! yellow
*.color3:       #d79921
*.color11:      #fabd2f

! blue
*.color4:       #458588
*.color12:      #83a598

! magenta
*.color5:       #d65d0e
*.color13:      #fe8019

! cyan
*.color6:       #689d6a
*.color14:      #8ec07c

! white
